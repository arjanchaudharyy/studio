# Required for ENG-104 end-to-end workflow
RUN_E2E=true

# OpenCode (Z.AI GLM-4.7)
ZAI_API_KEY=your_zai_api_key
# Optional overrides
#ZAI_BASE_URL=
#ZAI_HEADERS_JSON=

# AbuseIPDB / VirusTotal tools
ABUSEIPDB_API_KEY=your_abuseipdb_key
VIRUSTOTAL_API_KEY=your_virustotal_key

# AWS MCP tools (CloudTrail / CloudWatch)
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
# Optional (only if using temporary credentials)
#AWS_SESSION_TOKEN=
AWS_REGION=us-east-1

# Optional override of MCP images
#AWS_CLOUDTRAIL_MCP_IMAGE=shipsec/mcp-aws-cloudtrail:latest
#AWS_CLOUDWATCH_MCP_IMAGE=shipsec/mcp-aws-cloudwatch:latest
